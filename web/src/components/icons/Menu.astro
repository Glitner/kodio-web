---
import type { HTMLAttributes } from 'astro/types';

export type Props = HTMLAttributes<'svg'>;

const { width = '1rem', height = '1rem', ...rest } = Astro.props;
---

<svg
  {...rest}
  width={width}
  height={height}
  viewBox="0 0 16 16"
  stroke-width="2"
  stroke="currentColor"
  fill="none"
  stroke-linecap="round"
>
  <line x1="1" y1="2" x2="15" y2="2"></line>
  <line x1="1" y1="8" x2="15" y2="8"></line>
  <line x1="1" y1="14" x2="15" y2="14"></line>
</svg>
