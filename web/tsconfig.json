{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "lib/*": ["src/lib/*"],
      "types/*": ["src/types/*"],
      "utils/*": ["src/utils/*"]
    },
    "skipLibCheck": true,
    "types": ["astro/client"]
  }
}
