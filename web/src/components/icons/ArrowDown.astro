---
export type Props = astroHTML.JSX.SVGAttributes;

const { width = '1rem', height = '1rem', ...rest } = Astro.props;
---

<svg
  {...rest}
  width={width}
  height={height}
  viewBox="0 0 16 16"
  stroke-width="1.5"
  stroke="currentColor"
  fill="none"
  stroke-linecap="round"
>
  <line x1="8" y1="0" x2="8" y2="15"></line>
  <line x1="3" y1="10" x2="8" y2="15"></line>
  <line x1="8" y1="15" x2="13" y2="10"></line>
</svg>
